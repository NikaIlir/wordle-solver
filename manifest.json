{
  "manifest_version": 3,
  "name": "Wordle Solver",
  "description": "A simple wordle solver!",
  "version": "1.0.0",
  "icons": {
    "16": "w-icon.png",
    "32": "w-icon.png",
    "48": "w-icon.png",
    "128": "w-icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "w-icon.png",
      "32": "w-icon.png",
      "48": "w-icon.png",
      "128": "w-icon.png"
    }
  },
  "permissions": ["activeTab", "scripting"],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+U",
        "mac": "Command+U"
      }
    }
  },
  "content_scripts": [
    {
      "matches": ["https://*.nytimes.com/*"],
      "css": ["style.css"]
    }
  ]
}
